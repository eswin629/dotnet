@model InventoryViewModel
@{
    ViewData["Title"] = "Inventory Detail";
}

<h2>Inventory Detail</h2>
<table class="table tab-content">
    <tbody>
        <tr>
            <th>Id</th>
            <td>@Model.Id</td>
        </tr>
        <tr>
            <th>Name</th>
            <td>@Model.Name</td>
        </tr>

        <tr>
            <th>Merk</th>
            <td>@Model.Merk</td>
        </tr>
        <tr>
            <th>Tahun</th>
            <td>@Model.Tahun</td>
        </tr>
    </tbody>
</table>